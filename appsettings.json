{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "LogGroup": "WorkerService.LoggingDemo.LogGroup",
    "LogStreamNamePrefix": "dev",
    "Region": "ap-south-1",
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "C://temp//jtl_bihr_log_.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "Application": "WorkerService.LoggingDemo"
    }
  }
}
